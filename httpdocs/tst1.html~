		<script src="http://popcornjs.org/code/dist/popcorn-complete.js"></script>
       <script>
         document.addEventListener( "DOMContentLoaded", function() {
 
           var popcorn = Popcorn( "#ourvideo" );
 
           popcorn.footnote({
             start: 0,
             end: 1,
             target: "footnote",
             text: "1"
           });
 
            popcorn.footnote({
             start: 1,
             end: 2,
             target: "footnote",
             text: "2"
           });
           
           popcorn.footnote({
             start: 2,
             end: 3,
             target: "footnote",
             text: "3"
           });
           
           popcorn.footnote({
             start: 3,
             end: 4,
             target: "footnote",
             text: "4"
           });                      
         }, false );
         
       </script>		
    </head>
    <body>
